  "paths": {
    "/api/auth/getToken": {
      "post": {
        "tags": ["Auth"],
        "description": "get token",
        "responses": {
          "200": {
            "description": "OK",
            "schema": {
              "type": "object",
              "properties": {
                "message": {
                  "type": "string",
                  "example": "Token generated successfully"
                },
                "success": {
                  "type": "boolean",
                  "example": true
                },
                "data": {
                  "type": "string",
                  "example": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJpZCI6IjYyMDI4ZWZiMTI1ZDNhOWM2ZDZkOGQ0NSIsImlhdCI6MTY2NDY1NjQ5NCwiZXhwIjoxNjY0NjU2NDk0fQ.7dX6o9hGJkYl0VJpXW7i8JpXqS5w6Yh5"
                }
              }
            }
          }
        }
      }
    },
    "/api/users/": {
      "get": {
        "tags": ["User"],
        "description": "Get list of user",
        "parameters": [
          {
            "name": "Authorization",
            "in": "header",
            "type": "string",
            "default": "Bearer token"
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "schema": {
              "type": "object",
              "properties": {
                "message": {
                  "type": "string",
                  "example": "Users fetched successfully"
                },
                "success": {
                  "type": "boolean",
                  "example": true
                },
                "data": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "_id": {
                        "type": "string",
                        "example": "662028ef115d3aa9c6d68d45"
                      },
                      "userName": {
                        "type": "string",
                        "example": "nathan"
                      },
                      "accountNumber": {
                        "type": "string",
                        "example": "123456"
                      },
                      "emailAddress": {
                        "type": "string",
                        "example": "nathan@mail"
                      },
                      "identityNumber": {
                        "type": "string",
                        "example": "123456"
                      }
                    }
                  }
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized"
          }
        }
      }
    },
